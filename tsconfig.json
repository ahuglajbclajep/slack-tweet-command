{
  "compilerOptions": {
    // see https://github.com/cloudflare/workers-types/tree/v2.2.0#usage
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "webworker"],
    "strict": true,
    "moduleResolution": "node",
    "types": ["@cloudflare/workers-types"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
